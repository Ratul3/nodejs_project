<!DOCTYPE html>
<html>
<head>
	<title>LOGIN</title>
		<script type="text/javascript">
function validate()
{
	var name = document.forms["logform"]["uname"].value;
if(name==""){
	alert("Username Can't Be Empty");
return false;

}
var password = document.forms["logform"]["password"].value;

if(password==""){
	alert("Password Can't Be Empty");
return false;

}
	
	
}
</script>
</head>
<body>
		<form name="logform" onsubmit="return validate()" method="POST">
			<fieldset>
				<h1>LOGIN</h1>
				<table>
					<tr>
						<th>Username</th>
						<td><input type="text" name="uname"></td>
					</tr>
					<tr>
						<th>Password</th>
						<td><input type="password" name="password"></td>
					</tr>
					<tr>
						
						<td><input type="submit" name="submit" value="Submit"></td>
						<td><a href="/registration">Registration</a></td>
					</tr>
				</table>
			</fieldset>
		</form>
</body>
</html>